{
  "name": "chainbrary",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:localize": "ng build --localize",
    "build:test": "ng build --configuration test",
    "build:test:localize": "ng build --configuration test --localize",
    "build:prod": "ng build --configuration production",
    "build:prod:localize": "ng build --configuration production --localize",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "cy:run": "cypress run",
    "test-e2e:local": "cypress open",
    "test-e2e:ci": "start-server-and-test start $URL cy:run",
    "test-unit:local": "ng test",
    "test-unit:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "build:libs": "./scripts/build_libraries.sh",
    "publish:libs": "./scripts/publish_libraries.sh",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "translate": "node --loader ts-node/esm scripts/translation.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.12",
